<% @codenames.each do |codename, dist| -%>
Origin: <%= node[:reprepro][:fqdn] %>
Label: apt repository
Codename: <%= codename %>
Architectures: <%= @architectures.join(" ") %>
Components:  <%= dist["components"].join(" ") %>
Description: <%= dist["description"] || node[:reprepro][:description] %>
<%- unless dist["suites"].nil? -%>
Suite: <%= dist["suites"].join(" ") %>
<%- end -%>
SignWith: <%= node[:reprepro][:pgp_email] %>
<%   unless @pulls["name"] == codename -%>
Pull: <%= codename %>
Desindices: Sources  Release . .gz /usr/bin/rredtool
Desindices: Packages Release . .gz /usr/bin/rredtool
<%   end -%>

<% end -%>
